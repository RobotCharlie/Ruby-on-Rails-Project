<%= error_messages_for(@subject) %>

<table summary="Subject form fields">
  <tr>
    <!--<th>Name</th>-->
    <th><%= f.label(:name, "Name") %></th>
    <td><%= f.text_field(:name) %></td>
  </tr>
  <tr>
    <!--<th>Position</th>-->
    <th><%= f.label(:position, "Position") %></th>
    <!--<td><%#= f.text_field(:position) %></td>-->
    <td><%= f.select(:position, 1..@subject_count) %></td>
  </tr>
  <tr>
    <!--<th>Visible</th>-->
    <th><%= f.label(:visible, "Visible") %></th>
    <td><%= f.check_box(:visible) %></td>
  </tr>

  <tr>
    <th><%= f.label(:created_at)%></th>
    <td><%= f.datetime_select(:created_at, :order => [ :year ,:month, :day])%></td>
  </tr>
</table>