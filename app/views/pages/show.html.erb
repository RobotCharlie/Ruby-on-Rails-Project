
<% if !flash[:notice].blank? %>
    <div class="notice">
      <%= flash[:notice] %>
    </div>
<% end %>


<%= link_to(" << Back to List",{:action => 'index'}, :class => 'back-link')  %>

<div class="pages show">
  <h2>Show Page</h2>

  <table summary="Page detail view">
    <tr>
      <th>Position</th>
      <td><%= @page.position %></td>
    </tr>

    <tr>
      <th>Subject_ID</th>
      <td><%= @page.subject_id %></td>
    </tr>

    <tr>
      <th>Name</th>
      <td><%= @page.name %></td>
    </tr>

    <tr>
      <th>Permalink</th>
      <td><%= @page.permalink %></td>
    </tr>

    <tr>
      <th>Visible</th>
      <td><%= @page.visible %></td>
    </tr>

  </table>
</div>